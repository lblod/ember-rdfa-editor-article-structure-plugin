<AuList::Item>
  <AuButton
    @icon="add"
    @iconAlignment="left"
    @skin="link"
    @disabled={{false}}
    {{on 'click' this.insertArticle}}>
    {{t "insert.article"}}
  </AuButton>
</AuList::Item>
<AuList::Item>
  <AuButton
    @icon="add"
    @iconAlignment="left"
    @skin="link"
    @disabled={{this.isOutsideArticle}}
    {{on 'click' this.insertParagraph}}>
    {{t "insert.paragraph"}}
  </AuButton>
</AuList::Item>
<AuList::Item>
  <AuButton
    @icon="add"
    @iconAlignment="left"
    @skin="link"
    @disabled={{this.isOutsideArticle}}
    {{on 'click' (fn this.moveArticle true)}}>
    {{t "move.articleUp"}}
  </AuButton>
</AuList::Item>
<AuList::Item>
  <AuButton
    @icon="add"
    @iconAlignment="left"
    @skin="link"
    @disabled={{this.isOutsideArticle}}
    {{on 'click' (fn this.moveArticle false)}}>
    {{t "move.articleDown"}}
  </AuButton>
</AuList::Item>
<AuList::Item>
  <AuButton
    @icon="add"
    @iconAlignment="left"
    @skin="link"
    @disabled={{this.isOutsideParagraph}}
    {{on 'click' (fn this.moveParagraph true)}}>
    {{t "move.paragraphUp"}}
  </AuButton>
</AuList::Item>
<AuList::Item>
  <AuButton
    @icon="add"
    @iconAlignment="left"
    @skin="link"
    @disabled={{this.isOutsideParagraph}}
    {{on 'click' (fn this.moveParagraph false)}}>
    {{t "move.paragraphDown"}}
  </AuButton>
</AuList::Item>
{{#each this.structures as |structure|}}
  <AuList::Item>
    <AuButton
      @icon="add"
      @iconAlignment="left"
      @skin="link"
      @disabled={{false}}
      {{on 'click' (fn this.insertStructure structure.title)}}>
      Insert {{structure.title}}
    </AuButton>
  </AuList::Item>
{{/each}}
