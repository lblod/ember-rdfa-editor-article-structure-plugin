{{#each this.structures as |structure|}}
  <AuList::Item>
    <AuButton
      @icon="add"
      @iconAlignment="left"
      @skin="link"
      @disabled={{false}}
      {{on 'click' (fn this.insertStructure structure.title)}}>
      {{t structure.translation}}
    </AuButton>
  </AuList::Item>
{{/each}}
<AuList::Item>
  <AuButton
    @icon="add"
    @iconAlignment="left"
    @skin="link"
    @disabled={{false}}
    {{on 'click' this.insertArticle}}>
    {{t "articleStructurePlugin.insert.article"}}
  </AuButton>
</AuList::Item>
<AuList::Item>
  <AuButton
    @icon="add"
    @iconAlignment="left"
    @skin="link"
    @disabled={{this.isOutsideArticle}}
    {{on 'click' this.insertParagraph}}>
    {{t "articleStructurePlugin.insert.paragraph"}}
  </AuButton>
</AuList::Item>
